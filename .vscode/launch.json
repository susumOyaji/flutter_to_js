{
    // IntelliSense を使用して利用可能な属性を学べます。
    // 既存の属性の説明をホバーして表示します。
    // 詳細情報は次を確認してください: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "flutter_jsonプログラムの起動",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/web/jsto_flutter.js"
        },
        {
            "name": "flutter_to_js",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "flutter_to_js (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "flutter_to_js (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "type": "chrome",
            "name": "http://127.0.0.1:3000/web/index.html",
            "request": "launch",
            "url": "http://127.0.0.1:3000/web/index.html"
        }
    ]
}